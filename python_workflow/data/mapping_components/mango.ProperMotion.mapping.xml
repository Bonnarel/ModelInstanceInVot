<MODEL_INSTANCE>
	<MODELS>
		<MODEL>
			<NAME>ivoa</NAME>
			<URL>http://www.ivoa.net/xml/VODML/20180519/IVOA-v1.0.vo-dml.xml
			</URL>
		</MODEL>
		<MODEL>
			<NAME>coords</NAME>
			<URL>https://volute.g-vo.org/svn/trunk/projects/dm/STC/Coords/vo-dml/STC_coords-v1.0.vo-dml.xml
			</URL>
		</MODEL>
	</MODELS>
	<GLOBALS>
	</GLOBALS>
	<TABLE_MAPPING tableref="Results">
		<INSTANCE dmrole="mango:MangoObject.parameters"
			dmtype="mango:Parameter">
			<ATTRIBUTE dmrole="mango:Parameter.semantic"
				dmtype="ivoa:string" value="#propermotion" />
			<ATTRIBUTE dmrole="mango:Parameter.ucd"
				dmtype="ivoa:string" value="pos.pm" />
			<ATTRIBUTE dmrole="mango:Parameter.description"
				dmtype="ivoa:string" value="calibrated" />

			<INSTANCE dmrole="mango:Parameter.measure" dmtype="meas:ProperMotion">
				<INSTANCE dmrole="meas:ProperMotion.lon"
					dmtype="coords:PhysicalCoordinate">
					<INSTANCE dmrole="coords:PhysicalCoordinate.cval"
						dmtype="ivoa:RealQuantity">
						<ATTRIBUTE dmrole="ivoa:RealQuantity.value"
							dmtype="ivoa:real" ref="@@@@@@" />
						<ATTRIBUTE dmrole="ivoa:Quantity.unit"
							dmtype="ivoa:Unit" ref="@@@@@@" />
					</INSTANCE>
				</INSTANCE>
				<INSTANCE dmrole="meas:ProperMotion.lat"
					dmtype="coords:PhysicalCoordinate">
					<INSTANCE dmrole="coords:PhysicalCoordinate.cval"
						dmtype="ivoa:RealQuantity">
						<ATTRIBUTE dmrole="ivoa:RealQuantity.value"
							dmtype="ivoa:real" ref="@@@@@@" />
						<ATTRIBUTE dmrole="ivoa:Quantity.unit"
							dmtype="ivoa:Unit" ref="@@@@@@" />
					</INSTANCE>
				</INSTANCE>
				<INSTANCE dmrole="meas:Measure.error" dmtype="meas:Error">
					<INSTANCE dmrole="meas:Error.statError" dmtype="Ellipse">
						<COLLECTION size="2" dmrole="meas:Ellipse.semiAxis">
							<INSTANCE dmrole="meas:Ellipse.semiAxis"
								dmtype="ivoa:RealQuantity">
								<ATTRIBUTE dmrole="ivoa:RealQuantity.value"
									dmtype="ivoa:real" ref="@@@@@@" />
								<ATTRIBUTE dmrole="ivoa:Quantity.unit"
									dmtype="ivoa:Unit" ref="@@@@@@" />
							</INSTANCE>
							<INSTANCE dmrole="meas:Ellipse.semiAxis"
								dmtype="ivoa:RealQuantity">
								<ATTRIBUTE dmrole="ivoa:RealQuantity.value"
									dmtype="ivoa:real" ref="@@@@@@" />
								<ATTRIBUTE dmrole="ivoa:Quantity.unit"
									dmtype="ivoa:Unit" ref="@@@@@@" />
							</INSTANCE>
						</COLLECTION>
						<INSTANCE dmrole="meas:Ellipse.posAngle"
							dmtype="ivoa:RealQuantity">
							<ATTRIBUTE dmrole="ivoa:RealQuantity.value"
								dmtype="ivoa:real" ref="@@@@@@" />
							<ATTRIBUTE dmrole="ivoa:Quantity.unit"
								dmtype="ivoa:Unit" ref="@@@@@@" />
						</INSTANCE>
					</INSTANCE>
				</INSTANCE>
			</INSTANCE>
		</INSTANCE>
	</TABLE_MAPPING>
</MODEL_INSTANCE>